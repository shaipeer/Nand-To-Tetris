/**
 * This chip is an extended DFF: remembers not only last input, but three 
 * a(t) = in(t-1) 
 * b(t) = in(t-2) 
 * c(t) = in(t-3) 
 */

CHIP DFF3Step {

    IN in[16];
	OUT a[16],b[16],c[16];

    PARTS:
	
	Register(in = in, load = true, out = a, out = R1OUT);
	Register(in = R1OUT, load = true, out = b, out = R2OUT);
	Register(in = R2OUT, load = true, out = c);
	
	
	
}
